{
  "name": "@monstrs/mtproto-tl-to-typescript",
  "version": "0.0.12",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "bin": {
    "tl-to-typescript": "./bin/tl-to-typescript.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@monstrs/mtproto-tl-core": "workspace:0.0.10",
    "@monstrs/mtproto-tl-json-parser": "workspace:0.0.2",
    "@monstrs/mtproto-tl-types": "workspace:0.0.2",
    "camelcase": "^8.0.0",
    "commander": "^12.0.0",
    "decamelize": "^6.0.0",
    "ts-morph": "^22.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@monstrs/mtproto-tl-primitives": "workspace:0.0.4"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
